{
  "name": "igreja-backend-render",
  "version": "1.0.0",
  "description": "Backend da Igreja para Render",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "build": "tsc || true",
    "postinstall": "npm run build && npx prisma generate && npx prisma db push && npx prisma db seed",
    "migrate": "npx prisma migrate deploy",
    "db:push": "npx prisma db push"
  },
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "dependencies": {
    "fastify": "^4.24.3",
    "@fastify/jwt": "^7.2.4",
    "@fastify/multipart": "^8.0.0",
    "dotenv": "^16.3.1",
    "@prisma/client": "^5.6.0",
    "bcryptjs": "^2.4.3",
    "zod": "^3.22.4",
    "typescript": "^5.2.2",
    "@types/node": "^20.8.10",
    "@types/bcryptjs": "^2.4.6"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "prisma": "^5.6.0"
  },
  "keywords": [
    "node",
    "render",
    "igreja",
    "backend",
    "typescript"
  ],
  "author": "Igreja",
  "license": "MIT",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
